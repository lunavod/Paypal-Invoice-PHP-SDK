{
    "name": "lunavod/paypal-invoice-sdk",
    "description": "PayPal's PHP SDK for Invoice REST APIs",
    "keywords": ["paypal", "payments", "rest", "sdk", "orders", "checkout"],
    "type": "library",
    "license": "https://github.com/lunavod/Paypal-Invoice-PHP-SDK/blob/master/LICENSE",
    "homepage": "https://github.com/lunavod/Paypal-Invoice-PHP-SDK/",
    "require": {
        "paypal/paypalhttp": "1.0.0"
    },
    "authors": [
        {
            "name": "Lunavod",
            "homepage": "https://github.com/lunavod/Paypal-Invoice-PHP-SDK/contributors"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "PayPalInvoiceSdk\\": "lib/PayPalInvoiceSdk"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\":"tests/"
        }
    },
    "scripts": {
        "unit": "phpunit --testsuite unit",
        "integration": "phpunit --testsuite integration"
    }
}
